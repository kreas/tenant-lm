# Admin password — set this to protect the admin panel
# Leave unset during local dev to skip auth
ADMIN_PASSWORD=

# Turso database (optional — defaults to local SQLite file)
# TURSO_DATABASE_URL=libsql://your-db.turso.io
# TURSO_AUTH_TOKEN=

# Cloudflare R2 storage (required for file uploads)
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_BUCKET_NAME=

# Webhook URL (optional) — POST submission data here on every form submit
# e.g. a Make.com or Zapier webhook URL
WEBHOOK_URL=
